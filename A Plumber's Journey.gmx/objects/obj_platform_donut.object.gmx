        <arguments>
          <argument>
            <kind>1</kind>
            <string>///Donut logic

//If not falling
if (vspeed == 0) {

    //If the player or a NPC is above, shake
    if (collision_rectangle(bbox_left,bbox_top-1,bbox_right,bbox_top,obj_player,0,0))
    &amp;&amp; (obj_player.y &lt; y-11)
    &amp;&amp; (obj_player.state &lt; 2)
    &amp;&amp; (obj_player.vspeed &gt;= 0)
        shake = 1;

    //Otherwise, wait to shake.
    else {
    
        shake = 0;
        alarm[0] = 30;
    }
}

//Otherwise
else {

    for (var i=0; i&lt;instance_count; i++;) {

        //If the player or a NPC is on this falling platform
        if (collision_rectangle(bbox_left,bbox_top-5,bbox_right,bbox_top+4,instance_id[i],0,0))
        &amp;&amp; (instance_id[i].y &lt; y-11)
        &amp;&amp; (instance_id[i].nograv == 0)
        &amp;&amp; (instance_id[i].vspeed &gt;= 0) {
    
            //Check if falling through a solidtop
            var check = collision_rectangle(instance_id[i].bbox_left,bbox_top-6,instance_id[i].bbox_right,bbox_top+1,obj_semisolid,0,1)
    
            //If not falling through, or falling through a lower donut, snap the player or a NPC vertically
            if (!check)
            || ((check.object_index = obj_platform_donut || check.object_index = obj_platform_donut_triple) &amp;&amp; (check.y &gt; y || (check.y = y &amp;&amp; check &gt; id)))
                instance_id[i].y = ceil(bbox_top-16);            
        }
    }

    //Do not shake
    shake = 0;
    
    //Cap vertical speed.
    if (vspeed &gt; 2)
        vspeed = 2;
        
    //If the platform start position is outside the view.
    if (y &gt; room_height)
        instance_destroy();
}
</string>
