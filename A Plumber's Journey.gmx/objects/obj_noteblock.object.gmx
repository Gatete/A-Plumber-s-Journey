            <string>///Note block logic (Player)

//If the player does exist and is overlapping this block from the top.
if (instance_exists(obj_player)) {

    if (collision_rectangle(bbox_left,bbox_top-5,bbox_right,bbox_top+4,obj_player,0,0))
    &amp;&amp; (obj_player.y &lt; y-11) 
    &amp;&amp; (top &gt; 0) {
        
        //Prevent the player from jumping
        obj_player.jumping = 2;
    
        //Stop moving
        obj_player.hspeed = 0;
                
        //Snap above the block
        if (vspeed &lt;&gt; 0) {
        
            obj_player.y = y-16;
            if (obj_player.x &lt; x+8)
                obj_player.x++;
            else
                obj_player.x--;
        }
    }
}
            
//Change the scale of the object.
if (vspeed != 0) {

    depth = -2;
    if (top == 0) {
    
        if (vspeed &lt; 0)
            scale += 0.05;
        else
            scale -= 0.05;
    }
    else {
        
        if (vspeed &lt; 0)
            scale -= 0.05;
        else
            scale += 0.05;    
    }
}
else {

    scale = 1;
    depth = -1;
}

//Make the solid move along with the block
if (top &gt; 0)
    mysolid.y = y;
