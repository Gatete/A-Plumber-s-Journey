            <kind>1</kind>
            <string>///Turn at enemies script

//Check for a nearby enemy
var overlap = collision_rectangle(bbox_left,bbox_top+4,bbox_right,bbox_bottom-1,obj_enemyparent,0,1);

//If a enemy is nearby and this object is overlapping it.
if (overlap) {

    //Go right if you are further to the right, or in the same spot and a higher id
    if (bbox_left+bbox_right)*0.5 &gt; (overlap.bbox_left+overlap.bbox_right)*0.5
    || ((bbox_left+bbox_right)*0.5 = (overlap.bbox_left+overlap.bbox_right)*0.5 &amp;&amp; (id &gt; overlap.id))
        hspeed = abs(hspeed);

    //Otherwise, go left
    else
        hspeed = -abs(hspeed);
}
