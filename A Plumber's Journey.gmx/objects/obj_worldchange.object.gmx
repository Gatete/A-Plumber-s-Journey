            <kind>1</kind>
            <string>///Change alpha

if (ready == 1) {

    alpha += 0.04;
    if (alpha &gt; 0.96) {
    
        //Set the new world
        global.world = myworld;
    
        //Set full alpha
        alpha = 1;
        
        //Fade out
        ready = 2;
        
        //Change music
        with (obj_mapplayer) alarm[3] = 1;
        
        //Change decoration
        with (obj_mapbackground) alarm[0] = 1;
        with (obj_mapdecoration) alarm[0] = 1;
    }
}
else {

    alpha -= 0.04;
    if (alpha &lt; 0.04) {
    
        alpha = 0;
        ready = 0;
    }
}
</string>
