            <string>///Podoboo logic

//Make it fall when reaching start position.
if ((y &lt; ystart+39) &amp;&amp; (ready == true)) {

    //Make it fall.
    ready = false;
    gravity = 0.2;
}

//Bounce if outside the room.
if (bbox_top &gt; room_height) {

    if (!ready) {
    
        //Make it stop
        ready = true;
        
        //Stop after 10 steps.
        alarm[0] = 10;
    }
}

//Bounce in contact with lava.
lava = collision_rectangle(bbox_left,bbox_bottom,bbox_right,bbox_bottom,obj_lava,0,0);
if (lava) {

    //Bounce.
    if ((!ready) &amp;&amp; (vspeed &gt; 0)) {

        //Make it stop
        ready = true;
        
        //Stop after 10 steps.
        alarm[0] = 10;
        
        //Create lava splash
        repeat (6) {
        
            with (instance_create(x+8,lava.bbox_top,obj_sparkle_trail)) {
               
                //Motion
                motion_set(random_range(60,120),random_range(2,4));
                
                //Gravity
                gravity = 0.25;
                
                //Sprite
                sprite_index = spr_cinder;
            }
        }
    }
}

//Set the scale.
xscale = 1;

//Prevent it from falling too fast.
if (vspeed &gt; 6)
    vspeed = 6;
