            <kind>1</kind>
            <string>///Finish sprouting

//Create a item out of the sprite.
switch (sprite_index) {

    //Mushroom
    case (spr_mushroom): {
    
        //Create a mushroom
        with (instance_create(x,y,obj_mushroom)) {
        
            if (other.hspeed &gt; 0)
                hspeed = 1;
            else if (other.hspeed &lt; 0)
                hspeed = -1;
        }
    } break;
    
    //Fire flower
    case (spr_fireflower): {
    
        //Create a fire flower
        instance_create(x,y,obj_fireflower);
    } break;
    
    //Star
    case (spr_star): {
    
        //Create a star
        with (instance_create(x,y,obj_star)) {
        
            //Set the vertical speed
            if (other.vspeed &lt; 0)           
                vspeed = -4;
            
            //Set the horizontal speed
            if (other.hspeed &gt; 0)
                hspeed = 1;
            else if (other.hspeed &lt; 0)
                hspeed = -1;            
        }
    } break;
    
    //1-Up
    case (spr_1up): {
    
        //Create a 1up mushroom
        with (instance_create(x,y,obj_1up)) {
        
            if (other.hspeed &gt; 0)
                hspeed = 1;
            else if (other.hspeed &lt; 0)
                hspeed = -1;
        }
    } break;
    
    //Poison
    case (spr_poison): {
    
        //Create a poison mushroom
        with (instance_create(x,y,obj_poison)) {
        
            if (other.hspeed &gt; 0)
                hspeed = 1;
            else if (other.hspeed &lt; 0)
                hspeed = -1;
        }
    } break;
    
    //P-Switch
    case (spr_pswitch): {
    
        //Create a P-Switch
        instance_create(x,y,obj_pswitch);
    } break;
}

//Destroy
instance_destroy();
