          <argument>
            <kind>1</kind>
            <string>///Pipe player logic

//Update keys
scr_key_update();

//Update the player's palette
event_user(0);

//Update the player's sprite and boundary
event_user(1);

//Make the player exit the pipe.
if ((ready == 1) &amp;&amp; (!collision_rectangle(x-8,y+boundary,x+7,bbox_bottom,obj_solid,0,0))) {

    //Create player
    player = instance_create(x,y,obj_player);
    with (player) {
    
        //Hereby facing direction
        xscale = other.image_xscale;
        
        //If player is flashing, hereby the colour
        isflashing = obj_player_warp.isflashing;
    }
    
    //Destroy pipe player
    instance_destroy();
}

//Make the player move slower/faster.
if (collision_point(x-8,bbox_top,obj_solid,0,0))
&amp;&amp; (collision_point(x+7,bbox_top,obj_solid,0,0))
&amp;&amp; (collision_point(x-8,bbox_bottom,obj_solid,0,0))
&amp;&amp; (collision_point(x+7,bbox_bottom,obj_solid,0,0)) {

    speed = 2;
    visible = false;
}
else {

    //Play a sound if the player is about to exit a pipe.
    if ((ready == true) &amp;&amp; (ready2 == false)) {
    
        ready2 = true;
        scr_playsound(snd_powerdown, snd_powerdown_8bit, 0, false);
    }
    
    //Set the speed
    speed = 0.5;    
    
    //Make it visible
    visible = true;
}

//Change the facing direction of the player.
if (hspeed &gt; 0)
    image_xscale = 1;
else if (hspeed &lt; 0)
    image_xscale = -1;
</string>
