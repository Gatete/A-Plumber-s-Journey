        <arguments>
          <argument>
            <kind>1</kind>
            <string>///Unlock the door if the player has keys following

if (global.key != 0) {

    //Play 'Unlock' sound
    scr_playsound(snd_door_unlock, snd_door_unlock_8bit, 0, false);

    //Subtract 1 key
    global.key--;
    
    //Burst in a lot of sparkles
    var a = 0;
    var b = 22.5;
    repeat (8) {
    
        with (instance_create(x,y,obj_sparkle_trail)) {
        
            //Motion
            motion_set(a,1);
            
            //Sprite
            sprite_index = spr_sparkle_trail;
        }
        a += 45;
        
        with (instance_create(x,y,obj_sparkle_trail)) {
        
            //Motion
            motion_set(b,0.5);
            
            //Sprite
            sprite_index = spr_sparkle_trail;
        }
        b += 45;
    }
    
    //Remember it
    ds_map_add(global.keyget,id,1);
    
    //Create a new door object
    with (instance_create(x,y,obj_door)) {
    
        sprite_index = spr_door_key;
    }
    
    //Destroy
    instance_destroy();
}
</string>
