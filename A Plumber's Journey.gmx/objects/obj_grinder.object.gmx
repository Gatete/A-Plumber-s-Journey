            <kind>1</kind>
            <string>///Makes the grinder move around on paths.

//If the grinder is moving.
if (ready == 1) {

    //Check up in what direction is the grinder moving.
    switch (direct) {
    
        //If the grinder is moving to the right.
        case (0): {
        
            if (collision_point(x+1,y,obj_pathparent,1,0))
                x++;
            else if (collision_point(x+1,y-1,obj_pathparent,1,0)) {
            
                x++;
                y--;
            }
            else if (collision_point(x+1,y+1,obj_pathparent,1,0)) {
            
                x++;
                y++;
            }
            else if (collision_point(x,y-1,obj_pathparent,1,0)) {
            
                y--;
                direct = 90;
            }
            else if (collision_point(x,y+1,obj_pathparent,1,0)) {
            
                y++;
                direct = 270;
            }
            else            
                direct = 180;
        } break;
        
        //Otherwise, if the grinder is moving upwards.
        case (90): {
        
            if (collision_point(x,y-1,obj_pathparent,1,0))     
                y--;
            else if (collision_point(x-1,y-1,obj_pathparent,1,0)) {
            
                x--;
                y--;
            }
            else if (collision_point(x+1,y-1,obj_pathparent,1,0)) {
            
                x++;
                y--;
