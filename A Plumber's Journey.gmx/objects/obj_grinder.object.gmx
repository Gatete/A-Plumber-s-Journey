            else     
                direct = 270;
        } break;
        
        //Otherwise, if the grinder is moving to the left.
        case (180): {
        
            if (collision_point(x-1,y,obj_pathparent,1,0))         
                x--;
            else if (collision_point(x-1,y-1,obj_pathparent,1,0)) {
            
                x--;
                y--;
            }
            else if (collision_point(x-1,y+1,obj_pathparent,1,0)) {
            
                x--;
                y++;
            }
            else if (collision_point(x,y-1,obj_pathparent,1,0)) {
            
                y--;
                direct = 90;
            }
            else if (collision_point(x,y+1,obj_pathparent,1,0)) {
            
                y++;
                direct = 270;
            }
            else            
                direct = 0;
        } break;
        
        //Otherwise, if the grinder is moving downwards.
